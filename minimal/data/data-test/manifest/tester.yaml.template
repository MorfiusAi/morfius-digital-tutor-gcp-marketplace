    - name: "test"
      image: "us-east1-docker.pkg.dev/morfius-platform-402907/morfius-repo/morfius-test:v4"
      env:
      - name: "URL"
        valueFrom:
          configMapKeyRef:
            key: "URL"
            name: "morfius-tutor-config-ygfz"
      - name: "uri"
        valueFrom:
          configMapKeyRef:
            key: "uri"
            name: "morfius-tutor-config-ygfz"